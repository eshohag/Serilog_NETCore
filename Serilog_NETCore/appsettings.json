{  "AllowedHosts": "*",  "Serilog": {    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Console" ],    "MinimumLevel": {      "Override": {        "Microsoft": "Warning",        "System": "Warning"      }    },    "WriteTo": [      {        "Name": "Console",        "Args": {          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}] {Level:u3} {Username} {Message:lj}{Exception}, SourceContext:{SourceContext}, ActionName:{ActionName}{NewLine}"        }      },      {        "Name": "File",        "Args": {          "path": "/logs/log-.txt",          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}] {Level:u3} {Username} {Message:lj}{Exception}, SourceContext:{SourceContext}, ActionName:{ActionName}{NewLine}",          "fileSizeLimitBytes": 1000000, //1 MB Max File Size          "rollOnFileSizeLimit": "true",          "rollingInterval": "Day"        }      }    ],    "Enrich": [ "FromLogContext", "WithThreadId", "WithMachineName" ]  }}